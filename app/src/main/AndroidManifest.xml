<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.compshop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CompShop"
        tools:targetApi="31">
        <activity
            android:name=".QuienesSomos"
            android:exported="false"
            android:label="@string/title_activity_quienes_somos"
            android:theme="@style/Theme.CompShop.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".MecanicosActivity"
            android:exported="false" />
        <activity
            android:name=".DetallesMecanicoActivity"
            android:exported="false"
            android:label="@string/title_activity_detalles_mecanico"
            android:theme="@style/Theme.CompShop.NoActionBar" />
        <activity
            android:name=".cambio_aceite"
            android:exported="false" />
        <activity
            android:name=".acerca"
            android:exported="false"
            android:label="@string/title_activity_acerca"
            android:theme="@style/Theme.CompShop.NoActionBar" />
        <activity
            android:name=".Cotiza"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>